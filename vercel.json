{
    "redirects": [
      {
        "source": "/index.html",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/:path.html",
        "destination": "/:path",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/:path",
        "destination": "/:path.html"
      }
    ]
  }